{
    "env": {
        "browser": true,
        "es2021": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:import/typescript",
        "plugin:jasmine/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "jasmine",
        "@typescript-eslint"
    ],
    "rules": {
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
      }
    }
}
